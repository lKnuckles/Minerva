<cabal>
	<!-- This file defines default items, skills and quickslots of newly-created characters -->
	<init>
		<!--
			<item> elements have a fair few attributes you can set.  They are as follows:
			REQUIRED:
				class: The character class this item should be given to.
				id: The item ID.
				slot: The inventory slot to put the item in.
			OPTIONAL:
				amount: The amount of items to give the player.
				bonus: The bonus of the item (+1, +7, etc.).
				is_bound: Whether or not this item is account-bound (0 = false, 1 = true)			
				upgrades: The number of upgrade slots this item has.
				upgrade_x: The upgrade in slot x (where x = 1, 2, 3, or 4).
				craft_type: The type of this item's craft.
				craft_bonus: The bonus provided by this item's craft.
				expiration: The expiration type of this item.
		-->
		
		<item class="-1" id="3" amount="5" slot="0" />	
		<item class="-1" id="6" amount="20" slot="1" />
		<item class="-1" id="12" amount="5" slot="9" />	
		
		<item class="1" id="240" slot="2" />	
		<item class="2" id="270" slot="2" />		
		<item class="3" id="270" slot="2" />	
		<item class="4" id="255" slot="2" />
		<item class="5" id="240" slot="2" />	
		<item class="6" id="255" slot="2" />	
		<item class="7" id="240" slot="2" />
		
		<!--
			class: The character class this skill should be given to.
			id: The skill ID.
			level: The level of the skill.
			slot: The skill slot to put this skill in (sword skills start at 0, magic at 32).
			slot  67:  astral weapon(shield/bow)
			slot 104:  saint's force
		-->
		
		<skill class="-1" id="340" level="1" slot="72" />
		<skill class="-1" id="341" level="1" slot="73" />
		<skill class="-1" id="400" level="1" slot="75" />
